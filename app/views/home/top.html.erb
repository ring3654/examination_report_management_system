<h1>トップページ</h1>
<p><%= link_to '報告書の登録', home_report_top_path %></p>
<p><%= link_to '報告書の検索', home_retrieval_top_path %></p>
<p><%= link_to 'ログインユーザ－一覧', users_path %></p>
<p><%= link_to '報告書未承認一覧', unapproved_reports_index_path %></p>


<h2>未承認一覧</h2>
<table>
  <thead>
    <tr>
    <th width="40%">受験者ID</th>
    <th width="40%">学校名・企業名</th>
    <th width="20%"></th>
    </tr>
   </thead>
  </table>
 
 <div style="height:400px; overflow-y:scroll;">
  <table>

  <tbody>
      <% @ee_reports.each do |ee_report| %>
        <tr>
        <td width="40%"><%= ee_report.student_id %></td>
        <td width="40%"><%= ee_report.school_name %></td>
        <td width="20%"><%= link_to '閲覧', ee_report %></td>
        </tr>
      <% end %>
      <% @et_reports.each do |et_report| %>
          <tr>
          
          <td width="40%"><%= et_report.student_id %></td>
          <td width="40%"><%= et_report.office_name %></td>
          <td width="20%"><%= link_to '閲覧', et_report %></td>
          </tr>
        <% end %>
   </tbody>
  </table>
 </div>
